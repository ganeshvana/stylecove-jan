                                 <field name="selected_rooms"/>
                                 <field name="foyer"/>
                                 <field name="living"/>
                                 <field name="formal_family_room"/>
                                 <field name="kitchen"/>
                                 <field name="dining"/>
                                 <field name="crockery"/>
                                 <field name="bedrooms"/>
                                 <field name="vanity"/>
                                 <field name="toilet_vanity"/>
                                 <field name="devotional_place"/>
                                 <field name="study"/>
                                 <field name="balcony"/>
                                 <field name="utility"/>
                                 <field name="store_room"/>
                                 <field name="understairs_storage"/>
                                 <field name="wardrobe"/>
                                 <field name="tv_unit"/>
                                 <field name="remarks"/>

                            </group>
                        </group>
                    </xpath>
       
