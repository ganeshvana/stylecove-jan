                    <field name="ho_designer"/>
                    <field name="ho_lead_designer"/>
                    <field name="costing_manager"/>
                    <field name="installation_manager"/>
                    <field name="production_manager"/>
                
                  </field>
                   <xpath expr="//page[@name='lead']" position="attributes">    
                    <attribute name="string">Welcome Form</attribute>
                   </xpath>
                    <xpath expr="//page[@name='lead']" position="attributes">    
                    <attribute name="groups">oi_crm_inherit.group_front_desk</attribute>
                   </xpath>

                  <xpath expr="//form/sheet/group/group/div[2]" position="after">

                  <field name="branch_id"/>

                  </xpath>

                  <xpath expr="//form/sheet/notebook/page[@name='lead']/group/group/field[@name='website']" position="after">

                  <field name="end_to_end"/>

                  </xpath> 

                  <xpath expr="//form/sheet/notebook/page[@name='lead']/group/group/field[@name='website']" position="replace">

                  <field name="website"/>

                  </xpath> 

                  <xpath expr="//form/sheet/notebook/page[@name='lead']/group/group/field[@name='campaign_id']" position="before">

                  <field name="team_id"/>

                  </xpath>               

                  <xpath expr="//form/sheet/notebook/page[@name='lead']/group/group/div[2]" position="after">

                  <field name="site_location"/>
                  <field name="property_type"/>
                  <field name="property_remark"/>
                  <field name="no_of_bhk" string="No. of BHK"/>
                  <field name="bhk_remarks"/>
                  <field name="no_of_persons"/>
                  <field name="carpet_area_sqft"/>
                  <field name="status_of_property"/>
                  <field name="site_readiness_date"/>
                  <field name="interior_handover_date"/>
                  <field name="site_condition"/>

                  </xpath>

                   <xpath expr="//form/sheet/notebook/page[@name='lead']/group/group[@name='Misc']" position="attributes">
                    <attribute name="invisible">1</attribute>
                  </xpath>
                   <xpath expr="//form/sheet/notebook/page[@name='lead']/group/group[@name='Misc']" position="after">
                    <group>
                            <group string="SCOPE OF WORK" name="scope of work">
